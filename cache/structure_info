struct apev2_keys
{
	char title[5];
	char artist[6];
	char album[6];
	char year[4];
	char genre[5];
	char comment[7];
}

struct apev2_tag_item
{
	int32_t val_len;
	int32_t flags;
	char *item_key;
	int32_t key_size;//Not to be written in final tag
	char terminator;
	char *item_val;
	struct apev2_tag_item *next;//Next tag
}